{
	"schemaVersion": 1,
	"id": "examplemod",
	"version": "${version}",
	"name": "Create Wiki Example Mod",
	"description": "made to showcase various Create addon capabilities",
	"authors": [
		"snackbag wiki contributors"
	],
	"contact": {
		"homepage": "https://wiki.snackbag.net/w/cfa"
	},
	"license": "CC0-1.0",
	"icon": "assets/modid/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.example.modid.ExampleMod"
		],
		"fabric-datagen": [
			"com.example.modid.ExampleModDataGenerator"
